    <ChildTemplate id="1123" parentID="1093" level="2" creatorID="0" sortOrder="5" createDate="2015-09-18T14:29:58" updateDate="2015-10-14T11:40:06" nodeName="Hotel &amp; Travel" urlName="hotel-travel" path="-1,1093,1123" isDoc="" nodeType="1067" creatorName="Colin McFadden" writerName="Colin McFadden" writerID="0" template="1066" nodeTypeAlias="ChildTemplate">
