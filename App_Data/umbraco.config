      <ChildTemplate id="1470" parentID="1094" level="3" creatorID="9" sortOrder="12" createDate="2015-11-20T13:31:53" updateDate="2016-01-06T13:41:57" nodeName="Plant Tour" urlName="plant-tour" path="-1,1093,1094,1470" isDoc="" nodeType="1067" creatorName="Sarah Lunceford" writerName="Colin McFadden" writerID="0" template="1066" nodeTypeAlias="ChildTemplate">
