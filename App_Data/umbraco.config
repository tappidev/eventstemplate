      <ChildTemplate id="1470" parentID="1094" level="3" creatorID="9" sortOrder="11" createDate="2015-11-20T13:31:53" updateDate="2015-11-20T14:24:48" nodeName="Plant Tour" urlName="plant-tour" path="-1,1093,1094,1470" isDoc="" nodeType="1067" creatorName="Sarah Lunceford" writerName="Sarah Lunceford" writerID="9" template="1066" nodeTypeAlias="ChildTemplate">
